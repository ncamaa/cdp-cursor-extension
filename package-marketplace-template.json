{
  "name": "cursor-browser-inspector",
  "displayName": "Cursor Browser Inspector",
  "description": "Give Cursor AI direct access to Chrome DevTools. Automatically capture console logs, network requests, and errors for 10x faster debugging.",
  "version": "0.2.0",
  "publisher": "cursor-tools",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Debuggers",
    "Testing",
    "Other"
  ],
  "keywords": [
    "cursor",
    "chrome",
    "devtools",
    "debugging",
    "console",
    "network",
    "browser",
    "inspector",
    "ai",
    "assistant",
    "cdp",
    "chrome-devtools-protocol",
    "automatic-debugging",
    "network-requests",
    "console-logs",
    "error-tracking",
    "performance",
    "web-development",
    "frontend",
    "api-debugging"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cdp-debug.openChromeWithConnection",
        "title": "CDP: Open Chrome With Cursor Connection",
        "category": "CDP Debug"
      },
      {
        "command": "cdp-debug.stopConnection",
        "title": "CDP: Stop Connection",
        "category": "CDP Debug"
      },
      {
        "command": "cdp-debug.initProject",
        "title": "CDP: Initialize Project",
        "category": "CDP Debug"
      },
      {
        "command": "cdp-debug.openServerDashboard",
        "title": "CDP: Open Server Dashboard",
        "category": "CDP Debug"
      }
    ],
    "configuration": {
      "title": "CDP Browser Inspector",
      "properties": {
        "cdpDebug.autoInitProjects": {
          "type": "boolean",
          "default": true,
          "description": "Automatically prompt to initialize CDP Debug for new projects",
          "order": 1
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "chrome-remote-interface": "^0.33.3",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ncamaa/cdp-cursor-extension.git"
  },
  "bugs": {
    "url": "https://github.com/ncamaa/cdp-cursor-extension/issues"
  },
  "homepage": "https://github.com/ncamaa/cdp-cursor-extension#readme",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#0066FF",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/Cursor-AI%20Ready-blue",
      "href": "https://cursor.com",
      "description": "Cursor AI Ready"
    },
    {
      "url": "https://img.shields.io/badge/Protocol-MCP%20%2B%20HTTP-green",
      "href": "https://modelcontextprotocol.io",
      "description": "Supports MCP and HTTP"
    }
  ],
  "qna": "https://github.com/ncamaa/cdp-cursor-extension/discussions",
  "sponsor": {
    "url": "https://github.com/sponsors/ncamaa"
  }
}

