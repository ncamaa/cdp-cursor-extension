{
  "name": "cursor-browser-inspector",
  "displayName": "Cursor Browser Inspector",
  "description": "Give Cursor AI direct access to Chrome DevTools. Automatically capture console logs, network requests, and errors for 10x faster debugging. Now with browser automation!",
  "version": "0.3.0",
  "publisher": "codamasoftware",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Debuggers",
    "Other"
  ],
  "keywords": [
    "cursor",
    "chrome",
    "devtools",
    "debugging",
    "console",
    "network",
    "browser",
    "inspector",
    "ai",
    "assistant",
    "cdp",
    "chrome-devtools-protocol",
    "automatic-debugging",
    "network-requests",
    "console-logs",
    "error-tracking",
    "performance",
    "web-development",
    "frontend",
    "api-debugging",
    "mcp",
    "model-context-protocol"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cdp-debug.openChromeWithConnection",
        "title": "CDP: Open Chrome With Cursor Connection"
      },
      {
        "command": "cdp-debug.stopConnection",
        "title": "CDP: Stop Connection"
      },
      {
        "command": "cdp-debug.initProject",
        "title": "CDP: Initialize Project"
      },
      {
        "command": "cdp-debug.openServerDashboard",
        "title": "CDP: Open Server Dashboard"
      },
      {
        "command": "cdp-debug.copyMCPConfig",
        "title": "CDP: Copy MCP Configuration"
      }
    ],
    "configuration": {
      "title": "CDP Debug",
      "properties": {
        "cdpDebug.autoInitProjects": {
          "type": "boolean",
          "default": true,
          "description": "Automatically prompt to initialize CDP Debug for new projects"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package --no-dependencies"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "chrome-remote-interface": "^0.33.3",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "@vscode/vsce": "^2.22.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ncamaa/cdp-cursor-extension.git"
  },
  "bugs": {
    "url": "https://github.com/ncamaa/cdp-cursor-extension/issues"
  },
  "homepage": "https://github.com/ncamaa/cdp-cursor-extension#readme",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1a1d2e",
    "theme": "dark"
  }
}
